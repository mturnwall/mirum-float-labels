/**
 * Rules have 3 settings 0 (off), 1 (warning), 2 (error)
 */
{
  "extends": "mirum",
  "parserOptions": {
    "table": true
  },
  "rules": {
    "id-length": [
      2,
      {
        "exceptions": ["t"]
      }
    ],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "object-curly-newline": "off"
  }
}
