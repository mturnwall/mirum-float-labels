{
  "name": "mirum-float-labels",
  "version": "1.1.1",
  "description": "Have a form control label float inside the label. It moves out of the way when the control gets focus.",
  "main": "float-label.js",
  "config": {
    "jsOut": "dist/js",
    "cssMain": "float-label",
    "cssOut": "demo",
    "copyFiles": [
      "./src/float-label.scss"
    ]
  },
  "devDependencies": {
    "autoprefixer": "~7.1.1",
    "ava": "~0.24.0",
    "babel-core": "~6.24.0",
    "babel-loader": "~7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "~1.5.1",
    "browser-env": "^3.2.4",
    "chalk": "^1.1.3",
    "deepmerge": "^2.0.1",
    "eslint": "3.18.0",
    "eslint-config-mirum": "~1.3.0",
    "eslint-loader": "^1.8.0",
    "faucet": "0.0.1",
    "fs-extra": "^3.0.1",
    "gulp-extname": "^0.2.2",
    "hashmark": "~4.1.0",
    "mkdirp": "~0.5.1",
    "node-sass": "~4.5.1",
    "node-sass-globbing": "~0.0.23",
    "node-sass-magic-importer": "^5.0.0-alpha.15",
    "postcss": "~6.0.1",
    "postcss-cli": "~4.0.0",
    "rimraf": "~2.6.1",
    "rollup": "^0.54.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-uglify": "^3.0.0",
    "rsvp": "~3.5.0",
    "sassdoc": "~2.2.2",
    "sinon": "^4.1.4",
    "uglifyjs-webpack-plugin": "^1.1.6"
  },
  "scripts": {
    "build": "npm run lint && node bin/run build",
    "build:css": "node bin/run build-css",
    "build:js": "npm run lint && node bin/run build-js",
    "clean": "node bin/run clean",
    "copy": "node bin/run copy",
    "lint": "eslint -f 'table' src/*.js",
    "test": "ava --tap | faucet"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mturnwall/mirum-float-labels.git"
  },
  "keywords": [
    "mirum",
    "float-label"
  ],
  "author": "Michael Turnwall (Mirum, Inc)",
  "license": "MIT",
  "ava": {
    "babel": {
      "presets": [
        "env"
      ]
    },
    "require": [
      ".test/helpers/setup-browser-env.js"
    ]
  }
}
